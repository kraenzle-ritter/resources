{
  "name": "kraenzle-ritter/resources",
  "description": "Laravel package for managing external resources with Livewire components",
  "license": "MIT",
  "authors": [
    {
      "name": "Kraenzle, Andreas",
      "email": "kraenzle@k-r.ch",
      "homepage": "https://k-r.ch"
    }
  ],
  "homepage": "https://github.com/kraenzle-ritter/resources",
  "keywords": [
    "Laravel",
    "resources"
  ],
  "require": {
    "php": "^8.3",
    "survos/wikidata": "dev-master",
    "laravel/framework": ">=11",
    "guzzlehttp/guzzle": ">=7",
    "livewire/livewire": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": ">=11.5",
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "^9"
  },
  "autoload": {
    "psr-4": {
      "KraenzleRitter\\Resources\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KraenzleRitter\\Resources\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "KraenzleRitter\\Resources\\ResourcesServiceProvider"
      ],
      "aliases": {
        "Resources": "KraenzleRitter\\Resources\\Facades\\Resources"
      }
    }
  },
  "scripts": {
    "testbench:copy-views": "bash ./copy-views-testbench.sh",
    "test": [
      "@testbench:copy-views",
      "phpunit --configuration phpunit.xml"
    ]
  },
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ottosmops/wikidata.git"
    }
  ]
}
